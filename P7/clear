    <script>
jQuery(function () {
    var $button = jQuery('.endhome__refreshbtn');
    if ($button.length && de.bild.utils && de.bild.utils.viewportManager && de.bild.utils.viewportManager.triggerWhenInViewport) {
        $button.on('click', function () {
            try {
                // Tealium Event
                de.bild.events.publish('dataLayerEventTracking', ['click', {
                    click_name: 'vclick/hopu/button/homebot'
                }]);
            } catch (err) {}
            window.scrollTo(0, 0);
            window.location.reload();
        });
        de.bild.utils.viewportManager.triggerWhenInViewport($button, function () {
            try {
                // Tealium Event
                de.bild.events.publish('dataLayerEventTracking', ['view', {
                    click_name: 'view/hopu/button/homebot'
                }]);
            } catch (err) {}
        });
    }
});

